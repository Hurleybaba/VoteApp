{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "**/*.tsx",
    "**/*.jsx",
    "nativewind-env.d.ts",
    "app/(tabs)/menu.jsx",
    "app/(tabs)/home.jsx"
  ]
}
