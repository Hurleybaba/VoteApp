{
  "expo": {
    "name": "iElect",
    "slug": "ielect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo4.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo4.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hurleybaba.iElect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.hurleybaba.iElect"
    },
    "web": {
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo2.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/images/notification.wav"
          ]
        }
      ],
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "adcaf31d-39eb-4a3a-a839-01375358e1d1"
      }
    },
    "owner": "hurleybaba"
  }
}
